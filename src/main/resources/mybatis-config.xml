		<typeAlias type="com.example.boardinfo.model.gathering.dto.ChatRoomDTO" alias="chatRoomDTO"/>
		<typeAlias type="com.example.boardinfo.model.gathering.dto.GatheringAlarmDTO" alias="gatheringAlarmDTO"/>
		<typeAlias type="com.example.boardinfo.model.gathering.dto.AttendeeDTO" alias="attendeeDTO"/>


		<typeAlias type="com.example.boardinfo.model.tboard.dto.TBoardDTO" 
			alias="tboardDto"/>
		<typeAlias type="com.example.boardinfo.model.tboard.dto.TBCommentDTO"
			alias="tbCommentDto" />
		<typeAlias type="com.example.boardinfo.model.tboard.dto.TBAttachDTO"
			alias="tbFileDto"/>
		<typeAlias type="com.example.boardinfo.model.admin.dto.AdminDTO" alias="adminDto" />
	</typeAliases>

	<typeHandlers>
		<typeHandler handler="com.example.boardinfo.handler.AttendeeTypeHandler" />
	</typeHandlers>

</configuration>


